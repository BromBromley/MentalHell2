        <engine:VisualElement name="ExitPanel" enabled="true" style="align-self: center; justify-content: center; align-items: center; align-content: center; background-color: rgb(46, 46, 46); border-top-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; border-bottom-left-radius: 10px; overflow: visible; position: absolute; width: 15%; height: 210px; display: none;">
