<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/USS/MainUIStyle.uss?fileID=7433441132597879392&amp;guid=85d0a7847dfba6143af535ee264fb5e2&amp;type=3#MainUIStyle" />
    <engine:VisualElement name="MainPanel" style="flex-grow: 1; align-self: center; align-items: center; justify-content: center; opacity: 1; position: relative; top: auto; right: auto; align-content: center; flex-basis: auto; width: 100%; height: 100%; background-color: rgb(17, 6, 4);">
